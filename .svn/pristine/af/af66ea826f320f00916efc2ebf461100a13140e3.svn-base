Page({

    /**
     * 页面的初始数据
     */
    data: {
        _num: 4
    },
    //点击选项卡
    select: function(e){
        var item = e.target.dataset.id;
        this.setData({
            _num: item
        });
    },
    //滑动切换选项卡
    selec: function(e){
        var item = e.detail.current;
        this.setData({
            _num: item
        });
